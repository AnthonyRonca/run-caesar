n\n#game {\n  position: relative;\n  display: inline-grid;\n  background: black;\n}\n\n.game-status {\n  position: absolute;\n  top: 0;\n  left: 0;\n  display: table;\n  align-items: center;\n  justify-content: center;\n  width: 100%;\n  height: 50px;\n  border: 5px white solid;\n  background: black;\n  color: white;\n  font-family: Arial, Helvetica, sans-serif;\n  font-size: 1.5rem;\n}\n\n#score {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 200px;\n  height: 50px;\n  border: 5px red solid;\n  background: black;\n  margin-top: 20px;\n  color: white;\n  font-family: Arial, Helvetica, sans-serif;\n  font-size: 2rem;\n}\n\n#start-button {\n  margin-top: 20px;\n}\n\n.hide {\n  display: none;\n}\n\n.square {\n  display: block;\n}\n\n.blank {\n  background: black;\n}\n\n.wall {\n  border: 1px solid red;\n}\n\n.dot {\n  background: white;\n  border: 12px solid black;\n  border-radius: 50%;\n}\n\n.pill {\n  background: white;\n  border: 5px solid black;\n  border-radius: 50%;\n  animation: blinker 2s alternate infinite;\n}\n\n@keyframes blinker {\n  50% {\n    opacity: 0;\n  }\n}\n\n.ghost {\n  /* position: relative; */\n  /* background-image: url('./person.png') !important; */\n  background: url('./stabby.gif');\n  background-repeat: no-repeat;\n  background-size: 40px;\n  background-position: center;\n  border-radius: 50%;\n}\n\n.scared {\n  opacity: 0.5;\n  background-color: hsla(239, 67%, 67%, 0.3);\n}\n\n.blinky {\n  border: solid 3px red;\n}\n\n.pinky {\n  border: solid 3px pink;\n}\n\n.inky {\n  border: solid 3px cyan;\n}\n\n.clyde {\n  border: solid 3px orange;\n}\n\n.pacman {\n  background: url('./casear-walking.gif');\n  background-repeat: no-repeat;\n  background-size: 50px;\n  background-position: center;\n  border: solid 3px yellow;\n}\n\n.win-message {\n  float: center;\n  position: absolute;\n}\n\n.movie-container {\n  max-width: 100%;\n  max-height: 100%;\n}\n\n#modal {\n  display: none; /* Hidden by default */\n  position: fixed; /* Stay in place */\n  z-index: 1; /* Sit on top */\n  padding-top: 100px; /* Location of the box */\n  left: 0;\n  top: 0;\n  width: 100%; /* Full width */\n  height: 100%; /* Full height */\n  overflow: auto; /* Enable scroll if needed */\n  background-color: rgb(0,0,0); /* Fallback color */\n  background-color: rgba(0,0,0,0.4); /* Black w/ opacity */\n}\n\n/* Modal Content */\n.modal-content {\n  /* background-color: #fefefe; */\n  margin: auto;\n  padding: 20px;\n  width: 80%;\n}"},"lineCount":null}},"error":null,"hash":"93e9ab5262254e26dd8c18cff21cad43","cacheData":{"env":{}}}